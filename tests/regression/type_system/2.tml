
struct sttyp2 {
    char ?y.
}
struct styp {
    int ?c, ?z.
    sttyp2 ?inner, ?in3.
}
record father{symb ?a. symb ?b.}
record canFly{ symb ?c. }
record edge{int:3 ?c. int:2 ?c.}
record night{}
record pair{int ?a. int ?b.}
record school { styp ?name.  sttyp2 ?l.}

father(Tom Amy).   # Tom is a father of Amy.
canFly(bird).      # Bird can fly.
edge(6 3).         # Exists an edge from 6 to 3.
night.             # It's night.
pair(1 2).         # Exists a pair 1, 2.
edge(?x ?y) :- edge(?x ?z), edge(?x ?z).
school( notyet notyet).